{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/lino",
        "destination": "/lino/dist/",
        "type": 301
      },
      {
        "source": "/reader",
        "destination": "/reader/src/",
        "type": 301
      },
      {
        "source": "/",
        "destination": "/lino/dist/",
        "type": 301
      },
      {
        "source": "/noin",
        "destination": "/noin/",
        "type": 301
      },
      {
        "source": "/es6",
        "destination": "/ES6modules/",
        "type": 301
      }
    ]
  }
}
